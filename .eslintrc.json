{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": "off",
        "max-len": "off",
        "no-param-reassign": "off",
        "no-constant-condition": "off",
        "no-plusplus": "off",
        "no-confusing-arrow": "off",
        "no-else-return": "off"
    }
}